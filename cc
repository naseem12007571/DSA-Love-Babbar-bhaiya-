#include<bits/stdc++.h>
using namespace std;
int main()

{
    
    int n;
    cout<<" Enter the number check "<<endl;
    cin>>n;
    
    if(n>0){
        cout<<"number is positive "<<endl;
        
    } else if(n<0){
        cout<<" number is negative "<<endl;
    } else {
        cout<<" number is zero"<<endl;
    }
    
}
